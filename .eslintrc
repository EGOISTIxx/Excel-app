{
  // "eslint.validate": [ "javascript", "javascriptreact", "html", "typescriptreact" ],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "require-jsdoc":"off",
    "indent":"off",
    "eol-last":"off"
  },
  "extends": ["eslint:recommended", "google"]
}
